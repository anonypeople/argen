                        <specifier>final</specifier>
                        <name>short</name>
                    </type>
                    <name>OCCURS_ONE_OR_MORE</name> =
                    <init>
                        <expr>4</expr>
                    </init>
                </decl>
                ;
            </decl_stmt>

